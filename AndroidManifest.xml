<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.amazingpokharatour">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AmazingPokharaTour">
        <activity android:name=".SportsActivity"
            android:label="Sports"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".MusiumActivity"
            android:label="Musiums"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".CaveActivity"
            android:label="Caves and Place"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".LakeActivity"
            android:label="Lakes"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".TempleActivity"
            android:label="Temples"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".MountainActivity"
            android:label="Mountains"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>